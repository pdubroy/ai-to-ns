{
	"manifest_version": 2,
	"name": "AI to NS",
	"short_name": "ai-to-ns",
	"version": "1.1",
	"description": "Replaces the text 'artificial intelligence' with 'natural stupidity'.",
	"content_scripts": [
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_idle"
		}
	],
	"icons": {
    "48": "icon48.png",
    "128": "icon128.png"
   }
}
